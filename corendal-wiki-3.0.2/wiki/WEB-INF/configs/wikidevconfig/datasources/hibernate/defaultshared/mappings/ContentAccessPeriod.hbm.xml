<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.corendal.netapps.wiki.writedatamappings">
	<class name="DefaultContentAccessPeriodMapping"
		table="content_access_period">
		<id column="id" name="Id" type="string" />

		<property column="content_id" length="36" name="ContentId"
			not-null="true" type="string" />
		<property column="access_date" name="AccessDate" not-null="true"
			type="date" />
		<property column="access_num" name="AccessNumber"
			not-null="true" type="integer" />
		<property column="access_rank" name="AccessRank"
			not-null="false" type="integer" />
		<property column="enabled_flag" length="1" name="EnabledFlag"
			not-null="true" type="string" />
	</class>
	<query
		name="com.corendal.netapps.wiki.writedatabeans.AbstractContentAccessPeriod.findByContentPrimaryKey">
		<![CDATA[
    from DefaultContentAccessPeriodMapping
    where ContentId = :contentId
    order by AccessDate asc
]]>
	</query>
	<query
		name="com.corendal.netapps.wiki.writedatabeans.AbstractImage.findEnabledByRank.1">
		<![CDATA[
    select max(AccessDate) from DefaultContentAccessPeriodMapping
]]>
	</query>
	<query
		name="com.corendal.netapps.wiki.writedatabeans.AbstractArticle.findEnabledByRank.1">
		<![CDATA[
    select max(AccessDate) from DefaultContentAccessPeriodMapping
]]>
	</query>
	<query
		name="com.corendal.netapps.wiki.writedatafactories.ContentAccessPeriodFactory.refresh.1">
		<![CDATA[
    select max(AccessDate) from DefaultContentAccessPeriodMapping
]]>
	</query>
</hibernate-mapping>
