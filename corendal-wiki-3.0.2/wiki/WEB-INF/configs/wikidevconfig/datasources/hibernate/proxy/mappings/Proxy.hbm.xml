<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
	package="com.corendal.netapps.framework.core.writedatamappings">
	<class name="DefaultProxyMapping" table="proxy">
		<id column="id" name="Id" type="string" />

		<property column="app_id" length="36"
			name="AppId" not-null="true" type="string" />
		<property column="proxy_read_pref_id" length="36"
			name="ProxyReadPrefId" not-null="true" type="string" />
		<property column="enabled_flag" length="1" name="EnabledFlag"
			not-null="true" type="string" />
		<property column="proxy_email_pref_id" length="36"
			name="ProxyEmailPrefId" not-null="true" type="string" />
	</class>
</hibernate-mapping>
