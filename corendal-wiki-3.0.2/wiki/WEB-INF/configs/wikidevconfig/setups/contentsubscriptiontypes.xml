<contentsubscriptiontypes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../schemas/contentsubscriptiontypes.xsd">
    <contentsubscriptiontype enabled-flag="Y" id="WIKI-RSST-1" order="1">
        <name>Individual</name>
        <description>
            Subscriber can subscribe and unsubscribe
        </description>
        <comment />
    </contentsubscriptiontype>
    <contentsubscriptiontype enabled-flag="Y" id="WIKI-RSST-2" order="2">
        <name>Group</name>
        <description>
            Only the editor can subscribe and unsubscribe user accounts
        </description>
        <comment />
    </contentsubscriptiontype>
    <contentsubscriptiontype enabled-flag="Y" id="WIKI-RSST-3" order="3">
        <name>Individual and Group</name>
        <description>
            Subscriber is both an individual and a group subscriber
        </description>
        <comment />
    </contentsubscriptiontype>
</contentsubscriptiontypes>